{
  "name": "npm-script",
  "version": "0.0.2",
  "description": "",
  "main": "index.js",
  "config": {
    "port": "3000"
  },
  "scripts": {
    "lint": "npm-run-all --parallel lint:*",
    "lint:js": "eslint *.js",
    "lint:css": "stylelint *.less",
    "lint:json": "jsonlint --quiet *.json",
    "mocha": "mocha tests/",
    "eslint": "eslint *.js",
    "pretest": "npm run lint",
    "test": "mocha tests/",
    "precommit": "npm run lint",
    "prepush": "npm run test",
    "watch": "npm-run-all --parallel watch:*",
    "watch:lint": "onchange -i \"**/*.js\"  \"**/*.less\" -- npm run lint",
    "wtach:test": "npm t -- --watch",
    "cover": "node scripts/cover.js",
    "cover:serve": "scripty",
    "cover:open": "scripty",
    "client": "npm-run-all --parallel client:*",
    "client:reload-server": "livereload client/",
    "client:static-server": "http-server client/",
    "browser": "browser-sync start --server client/ --files \"**/*.css, **/*.js, **/*.html\""
  },
  "keywords": [],
  "author": "jianwang",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.26.5",
    "chai": "^4.2.0",
    "chalk": "^2.4.2",
    "cpr": "^3.0.1",
    "cross-var": "^1.1.0",
    "eslint": "^5.16.0",
    "http-server": "^0.11.1",
    "husky": "^2.1.0",
    "jsonlint": "^1.6.3",
    "lint-staged": "^8.1.5",
    "livereload": "^0.7.0",
    "make-dir-cli": "^2.0.0",
    "mocha": "^6.1.3",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.0.0",
    "onchange": "^5.2.0",
    "opn-cli": "^4.1.0",
    "rimraf": "^2.6.3",
    "scripty": "^1.9.1",
    "shelljs": "^0.8.3",
    "stylelint": "^10.0.1",
    "stylelint-config-standard": "^18.3.0"
  },
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      ".*.js"
    ]
  }
}
